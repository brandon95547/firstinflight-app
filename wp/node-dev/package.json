{
  "name": "raptor-web-solutions",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "npm run build-css && npm run js-compile",
    "build-dev": "npm run get-resources && npm run watch",
    "test": "mocha test -u bdd -R spec",
    "get-resources": "npm run clearFonts && npm run getGoogleFonts && npm run optimize-media",
    "watch": "nodemon --watch src -e scss,js --exec npm run build",
    "start-server": "babel-node app.js",
    "build-css": "npm run scss-compile && npm run css-autoprefix && npm run css-min",
    "clearFonts": "exec rm -rf public/fonts",
    "getGoogleFonts": "goofoffline outDir=../wp-content/themes/twentynineteen/css/fonts \"http://fonts.googleapis.com/css?family=Rock+Salt|Poppins|Roboto:300,400\"",
    "scss-compile": "node-sass src/sass/app.scss -o ../wp-content/themes/twentynineteen/css",
    "js-compile": "babel src/js --out-dir ../wp-content/themes/twentynineteen/js",
    "css-autoprefix": "npx postcss ../wp-content/themes/twentynineteen/css/app.css --use=autoprefixer --output=../wp-content/themes/twentynineteen/css/app.css",
    "css-min": "cleancss -o ../wp-content/themes/twentynineteen/css/app.min.css ../wp-content/themes/twentynineteen/css/app.css",
    "optimize-media": "imagemin --out-dir=../wp-content/themes/twentynineteen/img assets/img/**/*.{png,jpg,gif}"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "autoprefixer": "^9.7.2",
    "clean-css-cli": "^4.3.0",
    "concat": "^1.0.3",
    "google-fonts-offline": "^0.1.5",
    "imagemin-cli": "^5.0.0",
    "mocha": "^6.2.2",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "postcss-cli": "^6.1.3",
    "reload": "^3.0.3",
    "should": "^13.2.3",
    "uglify-js": "^3.6.9"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@wordpress/browserslist-config": "^2.6.0",
    "animate.css": "^3.7.2",
    "bootstrap": "^4.4.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "google-recaptcha": "^1.1.0",
    "http-errors": "~1.6.3",
    "jquery": "^3.5.0",
    "material-icons": "^0.3.1",
    "morgan": "~1.9.1",
    "slick-carousel": "^1.8.1"
  }
}
